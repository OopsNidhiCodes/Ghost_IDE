# GhostIDE Backend Configuration
# Copy this file to .env and update with your actual values

# Database Configuration
DATABASE_URL=postgresql+asyncpg://ghost:spooky@localhost:5432/ghostide

# Redis Configuration
REDIS_URL=redis://localhost:6379

# OpenAI API Configuration (Required for Ghost AI)
OPENAI_API_KEY=your-openai-api-key-here

# Ghost AI Configuration
GHOST_AI_MODEL=gpt-3.5-turbo
GHOST_AI_TEMPERATURE=0.7
GHOST_AI_MAX_TOKENS=500

# Security
SECRET_KEY=your-super-secret-key-here

# CORS Settings
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:5173"]

# Code Execution Settings
CODE_EXECUTION_TIMEOUT=30
MAX_CONCURRENT_EXECUTIONS=10

# Session Settings
SESSION_TIMEOUT_MINUTES=60